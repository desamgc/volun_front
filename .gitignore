spec/examples.txt
config/database.yml
config/secrets.yml
config/deploy-secrets.yml

/coverage

/config/beta-testers.txt
.byebug_history

# Mac finder artifacts
.DS_Store
.ruby-gemset

public/*
public/sitemap.xml
public/system/
